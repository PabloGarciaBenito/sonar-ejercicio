node {
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
    def mvn = tool 'jenkins-maven';
    withSonarQubeEnv(credentialsId: 'sonar') {
      sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=1a -Dsonar.projectName='1' -Dsonar.host.url=http://127.0.0.1:9000/"
    }
  }
}